{
  "name": "vi-nft-deploy-scripts",
  "description": "Deploy Casper NFT smart contracts using these scripts",
  "private": true,
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "install-contract": "sh -ac 'cp ./.env.$(git rev-parse --symbolic-full-name --abbrev-ref HEAD) .env; ts-node ./install.ts'",
    "install-profile": "sh -ac 'cp ./.env.$(git rev-parse --symbolic-full-name --abbrev-ref HEAD) .env; ts-node ./install_profile.ts'",
    "add-beneficiary": "sh -ac 'cp ./.env.$(git rev-parse --symbolic-full-name --abbrev-ref HEAD) .env; ts-node ./deployAddBeneficiary.ts'",
    "view-contract": "ts-node ./getDeployDetails.ts"
  },
  "keywords": [],
  "dependencies": {
    "casper-cep47-js-client": "^1.0.0",
    "casper-js-sdk": "^2.9.1",
    "dotenv": "^16.0.0",
    "secp256k1": "^4.0.3"
  }
}
